{
  "name": "qwen-vl-chat-app",
  "version": "1.0.0",
  "private": true,
  "description": "Qwen-VL多模态聊天应用",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "start:server": "node server/worker.js",
    "dev": "concurrently \"npm start\" \"npm run start:server\"",
    "start:multi": "cd server && node start.js",
    "start:workers": "cd server && node start-cluster.js",
    "start:balancer": "cd server && node load-balancer.js",
    "start:simple": "cd server && node simple-balancer.js",
    "stop:all": "taskkill /f /im node.exe",
    "clean": "taskkill /f /im node.exe && taskkill /f /im nginx.exe"
  },
  "keywords": [
    "chat",
    "ai",
    "react",
    "typescript",
    "qwen-vl",
    "multimodal"
  ],
  "author": "wenbo",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "clipboard": "^2.0.11",
    "form-data": "^4.0.5",
    "html2pdf.js": "^0.14.0",
    "http-proxy": "^1.18.1",
    "iconv-lite": "^0.7.2",
    "ioredis": "^5.9.2",
    "katex": "^0.16.27",
    "mammoth": "^1.11.0",
    "pdf-lib": "^1.17.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-katex": "^3.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/katex": "^0.16.8",
    "@types/node": "^20.19.30",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/react-katex": "^3.0.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^9.0.8", 
    "concurrently": "^9.2.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}